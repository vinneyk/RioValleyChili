using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.ComponentModel.DataAnnotations;
using Solutionhead.DataAnnotations;

namespace RioValleyChili.Data.Models
{
    public class ProductionBatchInstruction
